generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model groups {
  id   Int     @id @default(autoincrement())
  name String?
}

model products {
  id          Int     @id @default(autoincrement())
  description String?
  name        String?
  price       Float?
  qtdStock    Int?
  groupId     Int?
}

model orders {
  id           Int    @id @default(autoincrement())
  productId    Int
  quantity     Int?
  freightPrice Float?
}

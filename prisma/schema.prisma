generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model groups {
  id   Int     @id @default(autoincrement())
  name String?
}

model products {
  id          Int     @id @default(autoincrement())
  description String?
  name        String?
  price       Float?
  qtdStock    Int?
  groupId     Int?
  group       groups?  @relation(fields: [groupId], references: [id])
}
